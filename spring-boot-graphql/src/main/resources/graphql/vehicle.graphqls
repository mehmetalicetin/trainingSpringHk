type Vehicle{
    id: ID!,
    type: String,
    modelCode: String,
    brandName: String,
    launchDate: String
}

input VehicleDTO{
    type: String,
    modelCode: String,
    brandName: String
}

type Query {
    getVehicles(type: String) : [Vehicle]
    getById(id: ID) : Vehicle
}

type Mutation {
    createVehicle(vehicle : VehicleDTO): Vehicle
}